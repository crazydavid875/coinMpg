FROM nginx
RUN mkdir -p /var/www/html/member
RUN mkdir -p /var/www/html/member/back
RUN mkdir -p /var/www/html/itsa
COPY ./web/itsa /var/www/html/itsa/
COPY ./web/back /var/www/html/member/back
COPY ./ssl/*.crt /etc/nginx/ssl/nginx.crt
COPY ./ssl/*.key /etc/nginx/ssl/nginx.key
COPY nginx.conf /etc/nginx/conf.d/default.conf


